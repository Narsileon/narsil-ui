{
	"name": "narsil/ui",
	"description": "Narsil UI",
	"license": "MIT",
	"keywords": [
		"inertia",
		"laravel",
		"narsil",
		"php",
		"react",
		"ui"
	],
	"authors": [
		{
			"name": "Jonathan Rigaux"
		}
	],
	"repositories": [
		{
			"type": "path",
			"url": "../narsil-auth"
		},
		{
			"type": "path",
			"url": "../narsil-forms"
		},
		{
			"type": "path",
			"url": "../narsil-localization"
		},
		{
			"type": "path",
			"url": "../narsil-menus"
		},
		{
			"type": "path",
			"url": "../narsil-policies"
		},
		{
			"type": "path",
			"url": "../narsil-settings"
		},
		{
			"type": "path",
			"url": "../narsil-storage"
		},
		{
			"type": "path",
			"url": "../narsil-tables"
		},
		{
			"type": "path",
			"url": "../narsil-tree"
		}
	],
	"require": {
		"php": "^8.2",
		"inertiajs/inertia-laravel": "^1.3",
		"laravel/framework": "^11.3",
		"narsil/localization": "dev-main",
		"tightenco/ziggy": "^2.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^11.0"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Narsil\\UI\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Narsil\\UI\\NarsilUIServiceProvider"
			]
		}
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	}
}
