{
	"compilerOptions": {
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "preserve",
		"lib": ["dom", "dom.iterable", "esnext"],
		"module": "esnext",
		"moduleResolution": "node",
		"noEmit": false,
		"paths": {
			"@narsil-ui/*": ["./resources/js/*"],
			"vendor/*": ["./vendor/*"]
		},
		"resolveJsonModule": true,
		"skipLibCheck": false,
		"strict": true,
		"target": "esnext"
	},
	"include": ["resources/js/**/*.d.ts", "resources/js/**/*.ts", "resources/js/**/*.tsx"],
	"exclude": ["node_modules"]
}
